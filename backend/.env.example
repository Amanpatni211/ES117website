# ES117 Backend â€” Environment Variables
# Copy this file to .env and fill in your values
# cp .env.example .env

# Google OAuth (get from https://console.cloud.google.com)
ES117_GOOGLE_CLIENT_ID=your-client-id-here.apps.googleusercontent.com
ES117_GOOGLE_CLIENT_SECRET=your-client-secret-here

# Redirect URI (update with your actual server address)
ES117_GOOGLE_REDIRECT_URI=http://localhost:8000/api/auth/callback

# JWT secret (change this to a random string!)
ES117_JWT_SECRET=change-me-to-something-random

# Server port
ES117_PORT=8000

# Allowed frontend origins (comma separated)
ES117_ORIGINS=https://amanpatni211.github.io,http://localhost:3000,http://127.0.0.1:3000
